{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["es2015", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "lib",
    "skipLibCheck": true,
    "sourceMap": false,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5"
  },
  "typedoc": {
    "publishDir": "_react_docs",
    "include": [
      {
        "package": "@uirouter/core",
        "repo": "https://github.com/ui-router/core.git",
        "entry": "src/index.ts"
      }
    ],
    "generateOptions": {
      "tsconfig": "typedoc.json",
      "readme": "DOCS.md",
      "name": "@uirouter/react",
      "theme": "node_modules/ui-router-typedoc-themes/bin/default",
      "out": "_doc",
      "internal-aliases": "internal,publicapi,reactapi",
      "external-aliases": "external,internalapi",
      "navigation-label-globals": "@uirouter/react"
    }
  }
}
